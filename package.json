    "api",
    "proxy",
    "oauth"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "^5.7.0",
    "redis": "^5.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
