{
  "name": "claude-proxy-server",
  "version": "1.0.0",
  "description": "Claude API Proxy Server with OAuth support",
  "main": "dist/proxy-server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/proxy-server.js",
    "dev": "ts-node src/proxy-server.ts",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "forward": "node dist/forward-monitor.js",
